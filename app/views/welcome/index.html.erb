<%= form_tag listing_quandl_data_path, class: 'form-horizontal', method: 'post', remote: true do %>
  <div class="form-group">
    <label for="country" class="col-sm-2 control-label">Country</label>
    <div class="col-sm-10">USA</div>
  </div>
  <div class="form-group">
    <label for="name" class="col-sm-2 control-label">DATABASE</label>
    <div class="col-sm-10">
      <%= select_tag 'name', options_for_select([ ['STOCKS', 'stocks'], ['BONDS', 'bonds'] ]), class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <label for="name" class="col-sm-2 control-label">DATABASE CODE</label>
    <div class="col-sm-10 name_code">
      <%= select_tag 'code', options_for_select(['KFRENCH']), class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <label for="name" class="col-sm-2 control-label">&nbsp;</label>
    <div class="col-sm-10">
      <%= button_tag "Find Data", class: "btn btn-default submit-info", role: "button", data: { disable_with: "Please Wait...".html_safe} %>
    </div>
  </div>
<% end %>

<!-- Display Quandl Data -->
<hr>
<div id="quandl_data_info"></div>
